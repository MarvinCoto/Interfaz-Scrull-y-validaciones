<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <ScrollView
        android:layout_width="409dp"
        android:layout_height="729dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <Button
                android:id="@+id/btnRegistrarse"
                android:layout_width="135dp"
                android:layout_height="65dp"
                android:layout_marginStart="137dp"
                android:layout_marginTop="1156dp"
                android:layout_marginEnd="137dp"
                android:text="Registrarse"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <EditText
                android:id="@+id/txtNombre"
                android:layout_width="242dp"
                android:layout_height="43dp"
                android:layout_marginStart="84dp"
                android:layout_marginTop="30dp"
                android:layout_marginEnd="83dp"
                android:layout_marginBottom="35dp"
                android:ems="10"
                android:hint="Nombre"
                android:inputType="text"
                app:layout_constraintBottom_toTopOf="@+id/txtApellidos"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView2" />

            <TextView
                android:id="@+id/textView2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="99dp"
                android:layout_marginTop="76dp"
                android:layout_marginEnd="85dp"
                android:layout_marginBottom="16dp"
                android:text="Registro de Usuarios"
                android:textSize="24sp"
                android:textStyle="bold"
                app:layout_constraintBottom_toTopOf="@+id/txtNombre"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <EditText
                android:id="@+id/txtApellidos"
                android:layout_width="242dp"
                android:layout_height="44dp"
                android:layout_marginTop="36dp"
                android:ems="10"
                android:hint="Apellidos"
                android:inputType="text"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/txtNombre" />

            <EditText
                android:id="@+id/txtEdad"
                android:layout_width="243dp"
                android:layout_height="44dp"
                android:layout_marginStart="83dp"
                android:layout_marginTop="41dp"
                android:layout_marginEnd="83dp"
                android:layout_marginBottom="26dp"
                android:ems="10"
                android:hint="Edad"
                android:inputType="text"
                app:layout_constraintBottom_toTopOf="@+id/txtProfesion"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/txtApellidos" />

            <EditText
                android:id="@+id/txtProfesion"
                android:layout_width="242dp"
                android:layout_height="43dp"
                android:layout_marginStart="83dp"
                android:layout_marginEnd="84dp"
                android:layout_marginBottom="32dp"
                android:ems="10"
                android:hint="Profesion"
                android:inputType="text"
                app:layout_constraintBottom_toTopOf="@+id/txtTarjetaDeCredito"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent" />

            <EditText
                android:id="@+id/txtTarjetaDeCredito"
                android:layout_width="243dp"
                android:layout_height="47dp"
                android:layout_marginStart="83dp"
                android:layout_marginEnd="83dp"
                android:layout_marginBottom="26dp"
                android:ems="10"
                android:hint="Número de tarjeta de credito"
                android:inputType="text"
                app:layout_constraintBottom_toTopOf="@+id/txtTipoDeSangre"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent" />

            <EditText
                android:id="@+id/txtTipoDeSangre"
                android:layout_width="241dp"
                android:layout_height="44dp"
                android:layout_marginStart="83dp"
                android:layout_marginEnd="85dp"
                android:layout_marginBottom="27dp"
                android:ems="10"
                android:hint="Tipo de sangre"
                android:inputType="text"
                app:layout_constraintBottom_toTopOf="@+id/txtDireccion"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent" />

            <EditText
                android:id="@+id/txtDireccion"
                android:layout_width="240dp"
                android:layout_height="45dp"
                android:layout_marginStart="83dp"
                android:layout_marginEnd="86dp"
                android:layout_marginBottom="35dp"
                android:ems="10"
                android:hint="Dirección"
                android:inputType="text"
                app:layout_constraintBottom_toTopOf="@+id/txtEstadoCivil"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent" />

            <EditText
                android:id="@+id/txtEstadoCivil"
                android:layout_width="240dp"
                android:layout_height="43dp"
                android:layout_marginStart="84dp"
                android:layout_marginEnd="85dp"
                android:layout_marginBottom="38dp"
                android:ems="10"
                android:hint="Estado civil"
                android:inputType="text"
                app:layout_constraintBottom_toTopOf="@+id/txtTelefono"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent" />

            <EditText
                android:id="@+id/txtTelefono"
                android:layout_width="237dp"
                android:layout_height="46dp"
                android:layout_marginStart="83dp"
                android:layout_marginEnd="89dp"
                android:layout_marginBottom="34dp"
                android:ems="10"
                android:hint="Teléfono"
                android:inputType="text"
                app:layout_constraintBottom_toTopOf="@+id/txtPeso"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent" />

            <EditText
                android:id="@+id/txtPeso"
                android:layout_width="240dp"
                android:layout_height="42dp"
                android:layout_marginStart="83dp"
                android:layout_marginEnd="86dp"
                android:layout_marginBottom="29dp"
                android:ems="10"
                android:hint="Peso"
                android:inputType="text"
                app:layout_constraintBottom_toTopOf="@+id/txtNacionalidad"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent" />

            <EditText
                android:id="@+id/txtNacionalidad"
                android:layout_width="241dp"
                android:layout_height="49dp"
                android:layout_marginStart="84dp"
                android:layout_marginEnd="84dp"
                android:layout_marginBottom="31dp"
                android:ems="10"
                android:hint="Nacionalidad"
                android:inputType="text"
                app:layout_constraintBottom_toTopOf="@+id/txtAltura"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent" />

            <EditText
                android:id="@+id/txtAltura"
                android:layout_width="244dp"
                android:layout_height="47dp"
                android:layout_marginStart="83dp"
                android:layout_marginEnd="82dp"
                android:layout_marginBottom="28dp"
                android:ems="10"
                android:hint="Altura"
                android:inputType="text"
                app:layout_constraintBottom_toTopOf="@+id/txtDUI"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent" />

            <EditText
                android:id="@+id/txtDUI"
                android:layout_width="242dp"
                android:layout_height="45dp"
                android:layout_marginStart="84dp"
                android:layout_marginEnd="83dp"
                android:layout_marginBottom="38dp"
                android:ems="10"
                android:hint="DUI"
                android:inputType="text"
                app:layout_constraintBottom_toTopOf="@+id/btnRegistrarse"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent" />

        </androidx.constraintlayout.widget.ConstraintLayout>
    </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>